{
   "comment": "JSON Math Parser",

   "lex": {
      "rules": [
         ["\\s+",                    "/* skip whitespace */"],
         ["\\.",                    "return '.'"],
         ["[0-9]+(?:\\.[0-9]+)?\\b", "return 'NUMBER'"],
         ["\\\\",                    "/* skip whitespace */"],
         ["left",                    "/* skip whitespace */"],
         ["right",                    "/* skip whitespace */"],
         ["text",                    "return 'TEXT'"],
         ["=", "return '='"],
         ["\\<", "return '<'"],
         ["\\>", "return '>'"],
         ["\\*",                     "return '*'"],
         ["cdot",                     "return '*'"],
         ["\\/",                     "return '/'"],
         ["gcd",                     "return 'GCD'"],
         ["round",                     "return 'ROUND'"],
         ["min",                     "return 'MIN'"],
         ["max",                     "return 'MAX'"],
         ["frac",                     "return 'FRAC'"],
         ["-",                       "return '-'"],
         ["\\+",                     "return '+'"],
         ["sum",                     "return 'SUM'"],
         ["\\^",                     "return '^'"],
         ["sqrt",                     "return 'SQRT'"],
         ["!",                       "return '!'"],
         ["%",                       "return '%'"],
         ["\\(",                     "return '('"],
         ["\\{",                     "return '{'"],
         ["\\)",                     "return ')'"],
         ["\\}",                     "return '}'"],
         ["\\,",                     "return ','"],
         ["sin",                     "return 'sin'"],
         ["arcsin",                     "return 'arcsin'"],
         ["csc",                     "return 'csc'"],
         ["arccos",                     "return 'arccos'"],
         ["sec",                     "return 'sec'"],
         ["cos",                     "return 'cos'"],
         ["tan",                     "return 'tan'"],
         ["arctan",                     "return 'arctan'"],
         ["cot",                     "return 'cot'"],
         ["PI\\b",                   "return 'PI'"],
         ["pi",                   "return 'PI'"],
         ["E\\b",                    "return 'E'"],
         ["[a-zA-Z]+(?:\\[a-z]+)?\\b", "return 'VARIABLE'"],
         ["$",                       "return 'EOF'"]
      ]
   },

   "operators": [
      ["left", "+", "-"],
      ["left", "*", "/"],
      ["left", "^"],
      ["right", "!"],
      ["right", "%"],
      ["left", "UMINUS"],
      ["right", "=", "<", ">" ]
   ],

   "bnf": {
      "expressions": [["e EOF",   "return $1"]],

      "e" :[
         ["e + e",  "$$ = $1+$3"],
         ["e - e",  "$$ = $1-$3"],
         ["e * e",  "$$ = $1*$3"],
         ["e / e",  "$$ = $1/$3"],
         ["GCD ( e , e )",  "$$ = gcd( $3, $5 )"],
         ["ROUND ( e )",  "$$ = Math.round( $3 )"],
         ["MIN ( e , e )",  "$$ = Math.min( $3, $5 )"],
         ["MAX ( e , e )",  "$$ = Math.max( $3, $5 )"],
         ["FRAC { e } { e }",  "$$ = $3/$6"],
         ["TEXT { e }",  "$$ = $3"],
         ["e ^ e",  "$$ = Math.pow($1, $3)"],
         ["SQRT { e }",  "$$ = Math.sqrt( $3 )"],
         ["e !",    "$$ = (function(n) {if(n==0) return 1; return arguments.callee(n-1) * n})($1)"],
         ["e %",    "$$ = $1/100"],
         ["- e",    "$$ = -$2", {"prec": "UMINUS"}],
         ["( e )",  "$$ = $2"],
         ["NUMBER", "$$ = Number(yytext)"],
         ["E",      "$$ = Math.E"],
         ["PI",     "$$ = Math.PI"],
         ["sin ( e )",     "$$ = Math.sin( $3 )"],
         ["arcsin ( e )",     "$$ = Math.asin( $3 )"],
         ["csc ( e )",     "$$ = 1 / Math.sin( $3 )"],
         ["cos ( e )",     "$$ = Math.cos( $3 )"],
         ["arccos ( e )",     "$$ = Math.acos( $3 )"],
         ["sec ( e )",     "$$ = 1 / Math.cos( $3 )"],
         ["tan ( e )",     "$$ = Math.tan( $3 )"],
         ["arctan ( e )",     "$$ = Math.atan( $3 )"],
         ["cot ( e )",     "$$ = 1 / Math.tan( $3 )"],
         ["e = e", "$$ = ( $1 == $3 )"],
         ["e < e", "$$ = ( $1 < $3 )"],
         ["e > e", "$$ = ( $1 > $3 )"]
      ]
   }
}
